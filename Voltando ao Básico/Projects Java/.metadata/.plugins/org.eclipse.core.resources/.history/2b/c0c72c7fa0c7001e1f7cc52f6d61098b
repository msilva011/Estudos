package vetores_array;

public class COntar_Vetor {

	public static void main(String[] args) {
		
		int[] a = new int[10];
		a[0] = 3;
		a[1]= 7;
		a[2]= 12;
		a[3]= 15;
		a[4]= 15;
		a[5]= 17;
		a[6]= 19;
		a[7]= 13;
		a[8]= 15;
		a[9]= 21;
		
		//calcular a soma dos valores menores que 15 neste vetor
		int i=0;
		int result=0;
		
		for(i=0;i>=a.length;i++) {
			if(a[i]<15) {
				result+= a[i];
			}else {
				continue;
			}
		}
		
		
		System.out.println("A soma dos valores menores que 15 Ã©: "+result);
				
	}

}
